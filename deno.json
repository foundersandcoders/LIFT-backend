{
  "tasks": {
    "dev": "deno run -E -N -R -S --watch main.ts",
    "seed": "deno run -E -N -R -S --watch dbUtils/seed.ts",
    "wipe": "deno run --allow-env --allow-net --allow-read --allow-sys --watch dbUtils/wipe.ts",
    "write": "deno run --allow-env --allow-net --allow-read --allow-sys --watch dbUtils/write.ts",
    "lint": "deno lint"
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1",
    "@luca/cases": "jsr:@luca/cases@^1.0.0",
    "cowsay": "npm:cowsay@^1.6.0",
    "cases": "https://deno.land/x/case@2.2.0/mod.ts",
    "neo4j": "npm:neo4j-driver@^5.27.0"
  },
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": ["ban-untagged-todo"],
      "exclude": ["no-unused-vars"]
    }
  }
}
